{
  "name": "spotilyrics",
  "displayName": "Spotilyrics",
  "description": "See synchronized Spotify lyrics inside VS Code while coding",
  "version": "1.0.0-SNAPSHOT",
  "engines": {
    "vscode": "^1.39.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "spotilyrics.lyrics",
        "title": "Show Spotify Lyrics via Spotilyrics"
      },
      {
        "command": "spotilyrics.logout",
        "title": "Logout from Spotilyrics"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/colorthief": "^2.6.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.39.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "colorthief": "^2.6.0",
    "node-fetch": "^3.3.2",
    "ts-treemap": "^1.1.0",
    "tslib": "^2.8.1"
  },
  "license": "MIT"
}
